<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Text_Rabbit.Text_Rabbit.Text_Rabbit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.Text_Rabbit.Text_Rabbit.Text_Rabbit.MainActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Text_Rabbit.Text_Rabbit.Text_Rabbit.ModeSelect"
            android:screenOrientation="portrait"/>
        <activity android:name="com.Text_Rabbit.Text_Rabbit.Text_Rabbit.Highscores"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.Text_Rabbit.Text_Rabbit.Text_Rabbit.GameStandard"
            android:windowSoftInputMode="stateAlwaysVisible"
            android:screenOrientation="portrait"/>
        <activity android:name="com.Text_Rabbit.Text_Rabbit.Text_Rabbit.GameClassic"
            android:screenOrientation="portrait"/>
        <activity android:name="com.Text_Rabbit.Text_Rabbit.Text_Rabbit.Highscores_Standard"
            android:screenOrientation="portrait"/>
        <activity android:name="com.Text_Rabbit.Text_Rabbit.Text_Rabbit.Highscores_Classic"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>